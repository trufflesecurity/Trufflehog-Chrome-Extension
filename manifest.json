{
  "name": "Injecta",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Injecting stuff",
  "homepage_url": "http://danharper.me",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
		{
		 "matches": ["https://*/*","http://*/*"],
		 "js": ["inject.js"]
		}
	]
}
